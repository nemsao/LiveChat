{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleDirectories": ["node_modules","src"],
  "moduleNameMapper": {
    "^../__tests__/(.*)$": "<rootDir>/../__tests__/$1"
  },
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}
